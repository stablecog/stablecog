{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run i18n & vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "export $(xargs <.env) && node ./build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"update-types": "export $(xargs <.env) && npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/lib/ts/types/supabase.ts",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@egjs/svelte-infinitegrid": "^4.7.1",
		"@jsdevtools/rehype-toc": "^3.0.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.96",
		"@sveltejs/adapter-static": "^1.0.0-next.46",
		"@sveltejs/kit": "next",
		"@types/html-to-text": "^8.1.1",
		"@types/sharp": "^0.31.0",
		"@types/showdown": "^2.0.0",
		"@types/ua-parser-js": "^0.7.36",
		"@types/uuid": "^8.3.4",
		"autoprefixer": "^10.4.10",
		"bezier-easing": "^2.1.0",
		"html-to-text": "^8.2.1",
		"postcss": "^8.4.16",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"rehype-attr": "^2.1.1",
		"rehype-attributes": "^0.0.2",
		"rehype-external-links": "^2.0.1",
		"rehype-raw": "^6.1.1",
		"rehype-slug": "^5.0.1",
		"rehype-stringify": "^9.0.3",
		"remark-extract-frontmatter": "^3.2.0",
		"remark-frontmatter": "^4.0.1",
		"remark-images": "^3.1.0",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-copy": "^1.2.4",
		"svelte-intersection-observer": "^0.10.0",
		"svelte-local-storage-store": "^0.3.1",
		"svelte-preprocess": "^4.10.6",
		"svelte-textarea-autoresize": "^1.0.0",
		"tailwindcss": "^3.1.8",
		"tailwindcss-labeled-groups": "^0.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"unified": "^10.1.2",
		"vite": "^3.1.0",
		"vite-plugin-markdown": "^2.1.0",
		"yaml": "^2.1.3"
	},
	"type": "module",
	"dependencies": {
		"@ethercorps/sveltekit-og": "^0.1.8",
		"@supabase/auth-helpers-sveltekit": "^0.8.3",
		"@supabase/supabase-js": "^2.0.0",
		"aws-sdk": "^2.1243.0",
		"dexie": "^3.2.2",
		"sharp": "^0.31.1",
		"svelte-meta-tags": "^2.6.3",
		"typesafe-i18n": "^5.16.3",
		"ua-parser-js": "^1.0.2",
		"uuid": "^9.0.0"
